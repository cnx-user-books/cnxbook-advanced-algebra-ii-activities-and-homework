        <thead>
          <row>
            <entry><m:math><m:mi>x</m:mi></m:math></entry>
            <entry>
              <m:math>
                <m:semantics>
                  <m:mrow>
                    <m:mstyle fontsize="12pt">
                      <m:mrow>
                        <m:msup>
                          <m:mi>x</m:mi>
