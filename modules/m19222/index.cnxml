          <colspec colnum="2" colname="c2"/>
          <thead>
            <row>
              <entry>Speed (<m:math><m:mi>s</m:mi></m:math>)—miles per hour</entry>
              <entry>Time (<m:math><m:mi>t</m:mi></m:math>)—hours</entry>
            </row>
          </thead>
          <tbody>
            <row>
              <entry>50</entry>
              <entry/>
            </row>
            <row>
              <entry>75</entry>
              <entry/>
