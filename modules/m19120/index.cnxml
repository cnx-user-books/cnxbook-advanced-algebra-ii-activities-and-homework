                    </m:mrow>
                  </m:mrow>
                </m:mstyle>
                <m:mrow/>
              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{y=2+ {  {1}  over  {x} } } {}</m:annotation>
            </m:semantics>
