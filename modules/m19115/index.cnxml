          <m:annotation encoding="StarMath 5.0"> size 12{ \( 1,1 \)  \( 1, - 1 \)  \( 4,2 \)  \( 4, - 2 \)  \( 9,3 \)  \( 9, - 3 \) } {}</m:annotation>
        </m:semantics>
      </m:math> ________
    </item>
      <item><label>e</label>
      <m:math>
        <m:semantics>
          <m:mrow>
            <m:mstyle fontsize="12pt">
              <m:mrow>
                <m:mrow>
                  <m:mo stretchy="false">(</m:mo>
                  <m:mn>1,1</m:mn>
                  <m:mo stretchy="false">)</m:mo>
                  <m:mo stretchy="false">(</m:mo>
                  <m:mn>2,3</m:mn>
                  <m:mo stretchy="false">)</m:mo>
                  <m:mo stretchy="false">(</m:mo>
                  <m:mn>3,6</m:mn>
                  <m:mo stretchy="false">)</m:mo>
                  <m:mo stretchy="false">(</m:mo>
