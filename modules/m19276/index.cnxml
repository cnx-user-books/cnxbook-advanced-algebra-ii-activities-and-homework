      <exercise id="element-483"><problem id="idp12391120">
  <para id="element-961"><m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mfrac><m:mrow><m:mtext>28</m:mtext><m:msup><m:mi>k</m:mi><m:mstyle fontsize="8pt"><m:mrow><m:mn>3</m:mn></m:mrow></m:mstyle></m:msup><m:mrow><m:mi>p</m:mi><m:mo stretchy="false">−</m:mo><m:mtext>42</m:mtext></m:mrow><m:mrow><m:mstyle fontstyle="italic"><m:mrow><m:msup><m:mtext>kp</m:mtext><m:mstyle fontsize="8pt"><m:mrow><m:mn>2</m:mn></m:mrow></m:mstyle></m:msup></m:mrow></m:mstyle><m:mo stretchy="false">+</m:mo><m:mtext>56</m:mtext></m:mrow><m:mstyle fontstyle="italic"><m:mrow><m:msup><m:mtext>kp</m:mtext><m:mstyle fontsize="8pt"><m:mrow><m:mn>3</m:mn></m:mrow></m:mstyle></m:msup></m:mrow></m:mstyle></m:mrow><m:mrow><m:mtext>14</m:mtext><m:mstyle fontstyle="italic"><m:mrow><m:mtext>kp</m:mtext></m:mrow></m:mstyle></m:mrow></m:mfrac></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{ {  {"28"k rSup { size 8{3} } p - "42" ital "kp" rSup { size 8{2} } +"56" ital "kp" rSup { size 8{3} } }  over  {"14" ital "kp"} } } {}</m:annotation></m:semantics><m:mo>=</m:mo></m:math>
  </para>
</problem>

