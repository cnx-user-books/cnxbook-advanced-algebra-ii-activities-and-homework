            </m:mstyle>
            <m:mrow/>
          </m:mrow>
          <m:annotation encoding="StarMath 5.0"> size 12{6 lline 2x rSup { size 8{2} }  - "17"x - "85" rline +5 &gt;= 3} {}</m:annotation>
