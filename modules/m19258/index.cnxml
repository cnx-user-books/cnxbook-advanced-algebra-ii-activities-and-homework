                </m:mrow>
              </m:mrow>
            </m:mstyle>
            <m:mrow/>
          </m:mrow>
          <m:annotation encoding="StarMath 5.0"> size 12{x= - 3y rSup { size 8{2} } +5} {}</m:annotation>
        </m:semantics>
