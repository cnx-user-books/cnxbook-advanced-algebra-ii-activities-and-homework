              <m:mrow/>
            </m:mrow>
            <m:annotation encoding="StarMath 5.0"> size 12{ {  {2}  over  {x rSup { size 8{2} }  - 1} } + {  {x}  over  {x rSup { size 8{2} }  - 2x+1} } } {}</m:annotation>
