      <exercise id="id12353972">
        <problem id="idm6573536">
          <para id="id11986514"><m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mfrac><m:mn>4x</m:mn><m:mrow><m:mrow><m:mn>1</m:mn><m:mo stretchy="false">−</m:mo><m:mn>6</m:mn></m:mrow><m:mstyle fontstyle="italic"><m:mrow><m:mtext>ix</m:mtext></m:mrow></m:mstyle></m:mrow></m:mfrac><m:mo stretchy="false">−</m:mo><m:mfrac><m:mn>2i</m:mn><m:mrow><m:mn>3</m:mn><m:mo stretchy="false">−</m:mo><m:mi>i</m:mi></m:mrow></m:mfrac></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{ {  {4x}  over  {1 - 6 ital "ix"} }  -  {  {2i}  over  {3 - i} } } {}</m:annotation></m:semantics></m:math></para>

      <list id="id11729008" list-type="labeled-item" mark-suffix=".">
        <item><label>a</label><m:math>
<m:mi>a</m:mi>
<m:mo>=</m:mo>
</m:math></item>
        <item><label>b</label><m:math>
<m:mi>b</m:mi>
